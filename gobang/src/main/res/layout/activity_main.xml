<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:GobangAttrs="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.seniorcontrol.administrator.gobang.MainActivity">

    <com.seniorcontrol.administrator.gobang.GobangView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp"
        android:background="@drawable/bg"
        GobangAttrs:chessPiecesWidth="20dp"
        GobangAttrs:chessPiecesSpacing="30dp"
        GobangAttrs:horizontalGridCount="12"
        GobangAttrs:verticalGridCount="12"
        GobangAttrs:gridColor="#ff000000"
        GobangAttrs:gridWidth="5dp"
        />
</RelativeLayout>
